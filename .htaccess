ErrorDocument 404 https://elrctroweb.pxxl.click/404.php

RewriteEngine On


RewriteCond %{THE_REQUEST} \s/+(.+).php[\s?] [NC] RewriteRule ^ /%1 [R=301,L]


RewriteCond %{REQUEST_FILENAME} !-d RewriteCond %{REQUEST_FILENAME}.php -f RewriteRule ^(.*)$ $1.php [NC,L]


RewriteCond %{REQUEST_URI} !.php$ RewriteCond %{REQUEST_FILENAME} !-f RewriteCond %{REQUEST_FILENAME} !-d RewriteRule ^(.+)$ $1.php [NC,L]
